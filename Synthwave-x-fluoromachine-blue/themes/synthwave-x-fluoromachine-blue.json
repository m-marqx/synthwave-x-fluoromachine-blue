  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#3648A8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#2CC8E8"
      }
    },
    {
      "name": "Punctuation within templates",
      "scope": "string.template meta.embedded.line",
      "settings": {
        "foreground": "#C2AEC2"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#FFDC87"
      }
    },
    {
      "name": "Language variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#2CC8E8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage (declaration or modifier keyword)",
